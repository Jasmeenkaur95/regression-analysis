data=read.csv("Heart Failure Clinical Records.csv")
View(data)
model=lm(formula=age~diabetes+platelets,data=data)
summary(model)
model_A=lm(serum_sodium~serum_creatinine+smoking,data=data)
summary(model_A)
model_B=lm(smoking~time+sex+age,data=data)
summary(model_B)
model_C=lm(age~creatinine_phosphokinase+ejection_fraction+high_blood_pressure,data=data)
summary(model_C)
